scenarios:
- name: Send Synergy Test (Complete data, success)
  description: Tests the tag with all mandatory fields provided with valid data.
  code: |
    const mockData = {
      matchingDomain: 'ds1.nl',
      campaignId: 12345,
      synergyReference: 'REF-VALID-001'
    };

    let argUrl;
    const loggedMessages = [];

    mock('sendPixel', function(url, successCallback, failureCallback) {
        argUrl = url;
        successCallback();
    });
    mock('logToConsole', function() {
        let messageParts = [];
        for (let i = 0; i < arguments.length; i++) {
          messageParts.push(arguments[i]);
        }
        const fullMessage = messageParts.join(' ');
        loggedMessages.push(fullMessage);
    });

    runCode(mockData);

    // Asserts
    assertApi('sendPixel').wasCalled();
    assertThat(argUrl).isStrictlyEqualTo('https://ds1.nl/tr/?ci=12345&ref=REF-VALID-001&src=gtm-synergy%7C1.0');
    assertApi('gtmOnSuccess').wasCalled();
    assertApi('gtmOnFailure').wasNotCalled();
    assertThat(loggedMessages[0]).contains("Daisycon Synergy Tag: Attempting to send pixel to:");
