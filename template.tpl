___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const sendPixel = require('sendPixel');
const encodeUriComponent = require('encodeUriComponent');
const logToConsole = require('logToConsole');

/*
 * Build query string with object properties that are not undefined
 * @param {Object} requestParameters
 * @return string
 */
const buildQueryString = (requestParameters) => {
  let queryString = [];
  for (const parameter in requestParameters) {
	if (typeof requestParameters[parameter] !== 'undefined') {
	  queryString.push(parameter + '=' + encodeUriComponent(requestParameters[parameter]));
	}
  }

  return queryString.join('&');
};

// Check for null, undefined, or empty string (after trimming whitespace).
if (data.synergyReference === undefined || data.synergyReference === null || 
    (typeof data.synergyReference === 'string' && data.synergyReference.trim() === '')) {
  logToConsole('Daisycon Synergy Tag: ERROR - Mandatory synergyReference resolved to an empty or invalid value. Pixel not sent.');
  data.gtmOnFailure();
  return;
}

let path = '/tr/';
let requestParams = {
      ci: data.campaignId,
      ref: data.synergyReference,
      src: 'gtm-synergy|1.0.1'
    };

const baseUrl = 'https://' + data.matchingDomain.toString() + path;
const queryString = buildQueryString(requestParams);
const url = baseUrl + (queryString ? '?' + queryString : '');

logToConsole('Daisycon Synergy Tag: Attempting to send pixel to:', url);
sendPixel(url, data.gtmOnSuccess, data.gtmOnFailure);


